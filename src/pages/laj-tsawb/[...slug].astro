---
import { type CollectionEntry, getCollection } from "astro:content";
import BlogNkauj from "../../layouts/BlogNkauj.astro";
import RelatedSong from "../../components/RelatedSong.astro";
export const prerender = true;
export async function getStaticPaths() {
  const posts = await getCollection("laj-tsawb");
  return posts.map((post: any) => ({
    params: { slug: post.slug },
    props: post,
  }));
}
type Props = CollectionEntry<"laj-tsawb">;

const post = Astro.props;
const { Content } = await post.render();
---

<BlogNkauj {...post.data}>
  <Content />
  <RelatedSong name="laj-tsawb" />
</BlogNkauj>
